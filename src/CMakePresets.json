{
  "version": 6,
  "configurePresets": [
    {
      "name": "desktop",
      "hidden": true,
      "cacheVariables": {
        "NASTRO_OPT_DEV_BUILD": "OFF",
        "BUILD_SHARED_LIBS": "OFF",
        "BUILD_TESTING": "ON"
      }
    },
    {
      "name": "debug",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "CMAKE_DEBUG_POSTFIX": "d",
        "NASTRO_OPT_DEV_BUILD": "ON"
      }
    },
    {
      "name": "release",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "NASTRO_OPT_DEV_BUILD": "ON"
      }
    },
    {
      "name": "distro",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release"
      }
    },
    {
      "name": "linux",
      "hidden": true
    },
    {
      "name": "windows",
      "hidden": true
    },
    {
      "name": "desktop-debug-linux",
      "inherits": ["desktop", "debug", "linux"],
      "displayName": "Desktop-Debug-Linux",
      "cacheVariables": {

      }
    },
    {
      "name": "desktop-release-linux",
      "inherits": ["desktop", "release", "linux"],
      "displayName": "Desktop-Release-Linux"
    },
    {
      "name": "desktop-distro-linux",
      "inherits": ["desktop", "distro", "linux"],
      "displayName": "Desktop-Distro-Linux"
    },
    {
      "name": "desktop-debug-windows",
      "inherits": ["desktop", "debug", "windows"],
      "displayName": "Desktop-Debug-Windows"
    },
    {
      "name": "desktop-release-windows",
      "inherits": ["desktop", "release", "windows"],
      "displayName": "Desktop-Release-Windows"
    },
    {
      "name": "desktop-distro-windows",
      "inherits": ["desktop", "distro", "windows"],
      "displayName": "Desktop-Distro-Windows"
    }
  ]
}
